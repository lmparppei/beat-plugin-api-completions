{
	"name": "beat-api-completions",
	"displayName": "Autocompletion for Beat plugin API",
	"version": "0.0.1",
	"publisher": "com.kapitanFI",
	"private": true,
	"license": "GPL",
	"repository": {
		"type": "git",
		"url": "https://github.com/lmparppei/BeatPlugins"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.8.4"
	}
}
